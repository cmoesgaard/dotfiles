[core]
	editor = vim
	excludesfile = ~/.gitignore
[pull]
        rebase = true
[push]
	default = current
        autoSetupRemote = true
[fetch]
	prune = true
[rebase]
	autosquash = true
[alias]
	sw = switch
        co = checkout
        st = status
        br = branch
	lg = log --oneline -n 20 --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)'
[init]
	defaultBranch = main
[user]
	email = {{ .email }}
	name = Christoffer Moesgaard
